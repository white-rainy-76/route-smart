{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_PLATFORM": "all"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "env": {
          "EXPO_PUBLIC_PLATFORM": "ios"
        }
      },
      "android": {
        "env": {
          "EXPO_PUBLIC_PLATFORM": "android"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "env": {
          "EXPO_PUBLIC_PLATFORM": "ios"
        }
      },
      "android": {
        "env": {
          "EXPO_PUBLIC_PLATFORM": "android"
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "contact@fuelsmart.us",
        "ascAppId": "6756085554",
        "appleTeamId": "RJUJ9SWQG5"
      }
    }
  }
}
